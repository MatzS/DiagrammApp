<Window x:Class="De.HsFlensburg.DiagrammApp.Ui.Desktop.ChartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:De.HsFlensburg.DiagrammApp.Ui.Desktop"
        mc:Ignorable="d"
        Title="ChartWindow" Height="800" Width="1000"
        Loaded="Window_Loaded">
    <Window.DataContext>
        <Binding Source="{StaticResource ViewModelLocator}" Path="TheChartViewModel"></Binding>
    </Window.DataContext>
    <Grid>
        <Canvas Height="800" Width="1000" Name="canvas"/>
    </Grid>
</Window>
